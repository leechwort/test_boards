
(rules PCB display_pcb
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 1353)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.2)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.8)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_1000:500_um"
    (shape
      (circle F.Cu 1000.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 1000.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_1000:500_um" "Via[0-1]_1000:500_um" default
  )
  (via 
    "Via[0-1]_1000:500_um-kicad_default" "Via[0-1]_1000:500_um" "kicad_default"
  )
  (via 
    "Via[0-1]_1000:500_um-thick" "Via[0-1]_1000:500_um" thick
  )
  (via_rule
    default "Via[0-1]_1000:500_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_1000:500_um-kicad_default"
  )
  (via_rule
    thick "Via[0-1]_1000:500_um-thick"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    +3V3 GND VCC "Net-(C7-Pad2)" "Net-(C8-Pad2)" "Net-(D1-Pad1)" EN BS1
    BS2 CS RES DC RW RD D0 D1
    D2 D3 D4 D5 D6 D7 "Net-(R1-Pad2)" "Net-(P1-Pad19)"
    "Net-(P2-Pad4)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class thick
    (clearance_class thick)
    (via_rule thick)
    (rule
      (width 1000.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)